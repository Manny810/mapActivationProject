ort axios from \"axios\";\nimport { eventBus } from \"../main\";\n\nexport default {\n  name: \"CreateAccount\",\n  props: [],\n  components: {\n  },\n  data() {\n    return {\n      username: \"\",\n      password: \"\",\n    };\n  },\n  computed: {},\n  methods: {\n    createAccount() {\n      axios\n        .post(\"/api/user\", {\n          username: this.username,\n          password: this.password, \n        })\n        .then((response) => {\n          eventBus.$emit(\"account-creation-success\", {\n            data: response.data,\n          });\n          axios\n            .post(\"/api/session\", {\n            username: this.username,\n            password: this.password, \n            })\n            .then((response) => {\n            eventBus.$emit(\"login-success\", {\n                data: response.data,\n            });\n            this.$router.push(\"/\");\n            })\n            .catch((error) => {\n            eventBus.$emit(\"alert\", error.response.data.error);\n            });\n        })\n        .catch((error) => {\n          eventBus.$emit(\"alert\", error.response.data.error);\n        });\n    },\n  },\n};\n</script>\n\n<style></style>"]}]}