or\", (error) => {\n            this.response = error;\n            });\n            eventBus.$on(\"login-success\", (data) => {\n            this.signedInUser = data.data.username;\n            Vue.$cookies.set('username',data.data.username);\n            });\n            eventBus.$on(\"account-creation-success\", (data) => {\n            this.signedInUser = data.data.username;\n            Vue.$cookies.set('username',data.data.username);\n            });\n            eventBus.$on(\"log-out\", () => {\n            this.signedInUser = null;\n            Vue.$cookies.remove('username');\n            });\n        },\n        computed: {},\n        methods: {},\n    };\n</script>\n\n<style></style>"]}]}