","sourcesContent":["<template>\n  <div>\n    <form id=\"view-author\" v-on:submit.prevent=\"viewAuthor\">\n    <h2>View Freets By: </h2>\n    <div>\n        <label for=\"author\">Author:</label>\n        <input v-model=\"author\" id=\"author\" name=\"author\" />\n    </div>\n    <input type=\"submit\" value=\"View By Author\" />\n    </form>\n  </div>\n</template>\n\n<script>\nimport axios from \"axios\";\nimport { eventBus } from \"../main\";\n\nexport default {\n  name: \"ViewAuthor\",\n  props: [],\n  components: {\n  },\n  data() {\n    return {\n      author: \"\",\n    };\n  },\n  computed: {},\n  methods: {\n    viewAuthor() {\n        if (this.author === \"\"){\n            eventBus.$emit(\"alert\", \"Please Enter a Username\");\n        } else {\n      axios\n        .post(\"/api/freets/author\", {\n          author: this.author,\n        })\n        .then((response) => {\n          eventBus.$emit(\"response-change\", {\n            data: response.data,\n          });\n        })\n        .catch((error) => {\n          eventBus.$emit(\"alert\", error.response.data.error);\n        });\n        }\n    },\n  },\n};\n</script>\n\n<style></style>"],"sourceRoot":"src/components"}]}