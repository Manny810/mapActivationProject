YAAA,MAAA,GAAA,yCAAA,KAAA,CAAA,OAAA;AACA,QAAA,QAAA,CAAA,KAAA,CAAA,OAAA,EAAA,MAAA;AACA,QAAA,KAAA,CAAA,SAAA,GAAA,KAAA;AACA,OAPA,EAQA,KARA,CAQA,UAAA,KAAA,EAAA;AACA,QAAA,QAAA,CAAA,KAAA,CAAA,OAAA,EAAA,KAAA,CAAA,QAAA,CAAA,IAAA,CAAA,KAAA;AACA,OAVA;AAWA,KArBA;AAsBA,IAAA,WAtBA,yBAsBA;AAAA;;AACA,MAAA,KAAA,CAAA,MAAA,CAAA,iBAAA,KAAA,EAAA,EACA,IADA,CACA,YAAA;AACA,YAAA,MAAA,GAAA,gDAAA,MAAA,CAAA,OAAA;AACA,QAAA,QAAA,CAAA,KAAA,CAAA,OAAA,EAAA,MAAA;AACA,OAJA,EAKA,KALA,CAKA,UAAA,KAAA,EAAA;AACA,QAAA,QAAA,CAAA,KAAA,CAAA,OAAA,EAAA,KAAA,CAAA,QAAA,CAAA,IAAA,CAAA,KAAA;AACA,OAPA;AAQA;AA/BA;AAZA,CAAA","sourcesContent":["<template>\n  <div class=\"single-item\">\n    <div class=\"top-row\" >\n    @{{creator}}\n        <div class=\"buttons\" v-if=\"signedInUser==creator\">\n            <div v-if=\"edit_mode\">\n                <button v-on:click.prevent=\"finishEdit\">Finish</button>\n                <button v-on:click.prevent=\"cancelEdit\">Cancel</button>\n            </div>\n            <div v-else>\n                <button v-on:click.prevent=\"edit\">Edit</button>\n                <button v-on:click.prevent=\"deleteFreet\">X</button>\n            </div>\n        </div>\n    </div>\n    <div class=\"content\">\n        <div v-if=\"!edit_mode\">\n            {{content}}\n        </div>\n        <textarea v-if=\"edit_mode\" name=\"content\" v-model=\"content\"/>\n    </div>\n  </div>\n</template>\n\n<script>\nimport axios from \"axios\";\nimport { eventBus } from \"../main\";\n\nexport default {\n  name: \"SingleFreetView\",\n  props: [\"signedInUser\", \"creator\", \"input_content\", \"id\"],\n  data() {\n    return {\n        content: this.input_content || \"\",\n        message: \"\",\n        temp: \"\",\n        edit_mode: false,\n    };\n  },\n  created: function() {},\n  methods: {\n    edit() {\n        this.temp = this.content; \n        this.edit_mode = true; \n    },\n    cancelEdit() {\n      this.edit_mode = false; \n      this.content = this.temp; \n    },\n    finishEdit() {\n        axios.put(\"/api/freets/\" + this.id, {\n            content: this.content\n        })\n        .then(() => {\n            var output = \"Successfully changed your freet to: \" + this.content;\n            eventBus.$emit(\"alert\", output)\n            this.edit_mode = false; \n        })\n        .catch((error) => {\n            eventBus.$emit(\"alert\", error.response.data.error);\n        })\n    },\n    deleteFreet() {\n        axios.delete(\"/api/freets/\" + this.id)\n            .then(() => {\n                var output = \"Successfully deleted your following freet: \" + this.content; \n                eventBus.$emit(\"alert\", output)\n            })\n            .catch((error) => {\n                eventBus.$emit(\"alert\", error.response.data.error);\n            })\n    }\n\n  },\n};\n</script>\n\n<style>\ndiv.buttons {\n    margin-left:10px;\n}\ndiv.content {\n    margin-top:5px;\n    margin-bottom:10px; \n    margin-left:10px;\n    font-weight: bold;\n}\ndiv.top-row {\n    margin-top:10px; \n    margin-left:10px;\n    display:flex;\n}\ndiv.single-item {\n    border:1px solid white;\n}\nul {\n    list-style-type: none; \n}\n</style>"],"sourceRoot":"src/components"}]}