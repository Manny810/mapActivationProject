AAA,EAAA,UAAA,CAFA;AAGA,EAAA,UAAA,EAAA;AACA,IAAA,OAAA,EAAA,OADA;AAEA,IAAA,UAAA,EAAA,UAFA;AAGA,IAAA,aAAA,EAAA,aAHA;AAIA,IAAA,eAAA,EAAA;AAJA,GAHA;AASA,EAAA,IATA,kBASA;AACA,WAAA;AACA,MAAA,QAAA,EAAA,EADA;AAEA,MAAA,QAAA,EAAA;AAFA,KAAA;AAIA,GAdA;AAeA,EAAA,QAAA,EAAA,EAfA;AAgBA,EAAA,OAAA,EAAA;AACA,IAAA,MADA,oBACA;AAAA;;AACA,MAAA,KAAA,CACA,IADA,CACA,cADA,EACA;AACA,QAAA,QAAA,EAAA,KAAA,QADA;AAEA,QAAA,QAAA,EAAA,KAAA;AAFA,OADA,EAKA,IALA,CAKA,UAAA,QAAA,EAAA;AACA,QAAA,QAAA,CAAA,KAAA,CAAA,eAAA,EAAA;AACA,UAAA,IAAA,EAAA,QAAA,CAAA;AADA,SAAA;;AAGA,QAAA,KAAA,CAAA,OAAA,CAAA,IAAA,CAAA,GAAA;AACA,OAVA,EAWA,KAXA,CAWA,UAAA,KAAA,EAAA;AACA,QAAA,KAAA,CAAA,KAAA,CAAA;AACA,OAbA;AAcA;AAhBA;AAhBA,CAAA","sourcesContent":["<template>\n  <div>\n    <h1>Sign In Page</h1>\n    <h3 v-if=\"signedInUser\">\n      You are signed in as {{ signedInUser }}.<br />\n      You can safely go to the\n      <router-link class=\"router-link\" to=\"/\">home page</router-link>\n    </h3>\n    <div v-else>\n      <main class=\"two-col\">\n        <div>\n          <form id=\"sign-in\" v-on:submit.prevent=\"signIn\">\n            <div>\n              <label for=\"username\">Username:</label>\n              <input v-model=\"username\" id=\"username\" name=\"username\" />\n            </div>\n            <div>\n              <label for=\"password\">Password:</label>\n              <input v-model=\"password\" id=\"password\" name=\"password\" />\n            </div>\n            <input type=\"submit\" value=\"Sign In\" />\n          </form>\n          <CreateAccount />\n          <ViewAll />\n          <ViewAuthor />\n        </div>\n        <ResponseSection v-bind:response=\"response\" v-bind:signedInUser=\"signedInUser\"/>\n      </main>\n    </div>\n  </div>\n</template>\n\n<script>\nimport axios from \"axios\";\nimport ViewAll from \"../components/ViewAll.vue\";\nimport ViewAuthor from \"../components/ViewAuthor.vue\"\nimport CreateAccount from \"../components/CreateAccount.vue\";\nimport ResponseSection from \"../components/ResponseSection.vue\";\nimport { eventBus } from \"../main\";\n\nexport default {\n  name: \"SignIn\",\n  props: [\"signedInUser\", \"response\"],\n  components: {\n    ViewAll, \n    ViewAuthor, \n    CreateAccount, \n    ResponseSection,\n  },\n  data() {\n    return {\n      username: \"\",\n      password: \"\",\n    };\n  },\n  computed: {},\n  methods: {\n    signIn() {\n      axios\n        .post(\"/api/session\", {\n          username: this.username,\n          password: this.password, \n        })\n        .then((response) => {\n          eventBus.$emit(\"login-success\", {\n            data: response.data,\n          });\n          this.$router.push(\"/\");\n        })\n        .catch((error) => {\n          alert(error);\n        });\n    },\n  },\n};\n</script>\n\n<style></style>"],"sourceRoot":"src/views"}]}