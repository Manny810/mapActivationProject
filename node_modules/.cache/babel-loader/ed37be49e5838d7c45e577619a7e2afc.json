 v-on:submit.prevent=\"createFreet\">\n        <div>\n          <label for=\"content\">Your Freet:</label>\n          <input v-model=\"content\" id=\"content\" name=\"content\" />\n        </div>\n        <input type=\"submit\" value=\"Create Freet\" />\n      </form>\n  </div>\n</template>\n\n<script>\nimport axios from \"axios\";\nimport { eventBus } from \"../main\";\n\nexport default {\n  name: \"CreateFreet\",\n  props: [],\n  components: {\n  },\n  data() {\n    return {\n      content: \"\",\n    };\n  },\n  computed: {},\n  methods: {\n    createFreet() {\n      axios\n        .post(\"/api/freets\", {\n          content: this.content, \n        })\n        .then((response) => {\n          eventBus.$emit(\"response-change\", {\n            data: response.data,\n          });\n        })\n        .catch((error) => {\n          eventBus.$emit(\"alert\", error.response.data.error);\n        });\n    },\n  },\n};\n</script>\n\n<style></style>"],"sourceRoot":"src/components"}]}